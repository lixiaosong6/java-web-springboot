<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sale.dao.ChackInfoDao">

	<select id="getChackInfo" parameterType="com.sale.bean.ChackInfo"
		resultType="com.sale.bean.ChackInfo">
		SELECT *
		FROM chack_info where SALE_TYPE = #{sale_type}
	</select>

</mapper>
